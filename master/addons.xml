<?xml version='1.0' encoding='UTF-8'?>
<addons><addon id="repository.p1-ro.instant-kodi-repo" name="p1-ro/instant-kodi-repo Repository" provider-name="p1-ro" version="1.0.1">
    <extension point="xbmc.addon.repository">
        
        <dir minversion="17.0.0">
            <info compressed="false">https://p1-ro.github.io/instant-kodi-repo/master/addons.xml</info>
            <checksum>https://p1-ro.github.io/instant-kodi-repo/master/addons.xml.md5</checksum>
            <datadir zip="true">https://p1-ro.github.io/instant-kodi-repo/master/datadir/</datadir>
        </dir>

    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">A personal Kodi addon repository from https://github.com/p1-ro/instant-kodi-repo</summary>
        <description lang="en_GB" />
        <language />
        <platform>all</platform>
        <license>GPL-3.0</license>
        <forum />
        <source />
        <news />
        <disclaimer />
        <assets>
            <icon>resources/icon.png</icon>
            <fanart />
        </assets>
    </extension>
</addon><addon id="script.playonkodi.urldownloader" name="URL Downloader" provider-name="P1Ro" version="1.0.0">
